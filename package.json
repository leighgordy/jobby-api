{
  "name": "jobby-api",
  "version": "1.0.0",
  "description": "fetch client which allows access to jobby-rest restful service",
  "main": "lib/index.js",
  "types" : "lib/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leighgordy/jobby-api.git"
  },
  "author": "leighgordy@gmail.com",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/leighgordy/jobby-api/issues"
  },
  "homepage": "https://github.com/leighgordy/jobby-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "typescript": "^4.1.3"
  }
}
